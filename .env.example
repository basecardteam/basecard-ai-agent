# Gemini AI API Key
# Get from https://aistudio.google.com/
GEMINI_API_KEY=AIzaSy...your_key_here

# OpenRouter API Key (for Gemini 3 and other models)
# Get from https://openrouter.ai/
OPENROUTER_API_KEY=sk-or-v1-...your_key_here

# Neynar API Key for Farcaster data
# Get from https://neynar.com/
NEYNAR_API_KEY=your_neynar_api_key_here

# Neynar Daily Credit Limit (default: 10000)
# Free tier: ~3000, Paid plans vary
NEYNAR_DAILY_CREDIT_LIMIT=10000

# PostgreSQL Database URL (ai_agent_user)
# Format: postgres://user:password@host:port/database
DATABASE_URL=postgres://ai_agent_user:ai_agent_password@localhost:5432/basecard

# Server port
PORT=4000

# Test FID for running test scripts
# Example FIDs from your DB: 8106, 1051068, 1110391
TEST_FID=8106
